<animation-set>
    <animation id="POP_ANM_STAND" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_STAND" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_TURN">
        <sample-set>
            <sample sprite-group="POP_ANM_TURN" duration="0.13575" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_FALL" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_FALL" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_UP_START">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_UP_START" duration="0" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_UP" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_UP" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_HANG" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_HANG" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_CLIMB" rewind="1">
        <sample-set>
            <sample sprite-group="POP_ANM_CLIMB" duration="0.594" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_DESCEND">
        <sample-set>
            <sample sprite-group="POP_ANM_CLIMB" duration="0.594" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_FORWARD_START">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_FORWARD_START" duration="0.198" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_FORWARD" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_FORWARD" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_FORWARD_END">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_FORWARD_END" duration="0.198" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_WALK" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_WALK" duration="0.594" />
        </sample-set>
    </animation>
</animation-set>