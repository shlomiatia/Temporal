				<radius x="20.000000" y="120.000000"/>
			</obb>
		</fixture>
	</sensor>
	<sensor id="SNS_FRONT_EDGE" category-mask="3">
		<fixture>
			<obb angle="0">
				<center x="15" y="-45" />
				<radius x="5" y="5" />
			</obb>
		</fixture>
