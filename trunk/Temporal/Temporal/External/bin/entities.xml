        <scene-node id="SCN_BODY" sprite-sheet="skeleton.png">
            <scene-node id="SCN_LEFT_LEG" behind-parent="1">
                <scene-node id="SCN_LEFT_FOOT" />
            </scene-node>
            <scene-node id="SCN_LEFT_HAND" behind-parent="1">
                <scene-node id="SCN_LEFT_PALM" />
            </scene-node>
            <scene-node id="SCN_HEAD" />
            <scene-node id="SCN_RIGHT_LEG">
                <scene-node id="SCN_RIGHT_FOOT" />
            </scene-node>
            <scene-node id="SCN_RIGHT_HAND" >
                    <scene-node id="SCN_RIGHT_PALM" />
            </scene-node>
        </scene-node>
    </renderer>
</entity>
