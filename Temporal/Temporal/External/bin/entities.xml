<entity id="ENT_PLAYER">
    <transform>
        <position x="3000" y="512" />
    </transform>
    <collision-filter category="8" />
    <draw-position>
        <offset y="-40" />
    </draw-position>
    <input-controller />
    <dynamic-body>
        <fixture>
            <yabp y-radius="40">
                <sloped-radius x="10" />
            </yabp>
        </fixture>
    </dynamic-body>
    <sensor category-mask="1">
        <fixture>
            <yabp y-radius="40.5">
                <center y="80.5" />
                <sloped-radius x="9" />
            </yabp>
        </fixture>
        <contact-listener type="ledge-detector" id="SNS_HANG" />
    </sensor>
    <sensor category-mask="1">
        <fixture>
            <yabp y-radius="40.5">
                <center y="-80.5" />
                <sloped-radius x="9" />
            </yabp>
        </fixture>
        <contact-listener type="ledge-detector" id="SNS_DESCEND" />
    </sensor>
    <action-controller />
    <animator animation-set="POP_ANM_SET" />
    <renderer layer="4">
        <scene-node sprite-sheet="pop.png" />
    </renderer>
    <!--<particle-emitter lifetime="3" birth-threshold="0.5" sprite-sheet="bubble.png" birth-radius="50" velocity="50" center="180" size="180" />
    <player-period period="2" />-->
</entity>
