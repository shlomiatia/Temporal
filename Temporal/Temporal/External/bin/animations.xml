<animation-set id="CAM_ANM_SET">
    <animation id="CAM_ANM_SEARCH" repeat="1">
        <sample-set>
            <sample sprite-group="CAM_ANM_SEARCH" />
        </sample-set>
    </animation>
    <animation id="CAM_ANM_SEE" repeat="1">
        <sample-set>
            <sample sprite-group="CAM_ANM_SEE" />
        </sample-set>
    </animation>
    <animation id="CAM_ANM_TURN">
        <sample-set>
            <sample sprite-group="CAM_ANM_TURN" duration="0.2" />
        </sample-set>
    </animation>
    <animation id="CAM_ANM_TURN2" rewind="1">
        <sample-set>
            <sample sprite-group="CAM_ANM_TURN" duration="0.2" />
        </sample-set>
    </animation>
    <animation id="CAM_ANM_ACQUIRE" repeat="1">
        <sample-set>
            <sample sprite-group="CAM_ANM_SEARCH" duration="0.2" />
            <sample sprite-group="CAM_ANM_SEE" duration="0.2" />
        </sample-set>
    </animation>
</animation-set>
<animation-set id="SKL_ANM_SET">
    <animation id="SKL_ANM_FIGHT" repeat="1">
        <sample-set scene-node-id="SCN_BODY">
            <sample sprite-group="SCN_BODY" />
        </sample-set>
        <sample-set scene-node-id="SCN_HEAD">
            <sample sprite-group="SCN_HEAD">
                <translation x="9" y="31" />
            </sample>
        </sample-set>
        <sample-set scene-node-id="SCN_RIGHT_HAND">
            <sample sprite-group="SCN_RIGHT_HAND" duration="0.5" >
                <translation x="-4" y="19" />
            </sample>
            <sample sprite-group="SCN_RIGHT_HAND" duration="0.5" rotation="90">
                <translation x="-4" y="19" />
            </sample>
        </sample-set>
        <sample-set scene-node-id="SCN_RIGHT_PALM">
            <sample sprite-group="SCN_RIGHT_PALM">
                <translation x="-19" y="-38" />
            </sample>
        </sample-set>
        <sample-set scene-node-id="SCN_LEFT_HAND">
            <sample sprite-group="SCN_LEFT_HAND" duration="0.25" rotation="45">
                <translation x="28" y="28" />
            </sample>
            <sample sprite-group="SCN_LEFT_HAND" duration="0.5" rotation="90">
                <translation x="28" y="28" />
            </sample>
            <sample sprite-group="SCN_LEFT_HAND" duration="0.25">
                <translation x="28" y="28" />
            </sample>
        </sample-set>
        <sample-set scene-node-id="SCN_LEFT_PALM">
            <sample sprite-group="SCN_LEFT_PALM">
                <translation x="10" y="-26" />
            </sample>
        </sample-set>
        <sample-set scene-node-id="SCN_RIGHT_LEG">
            <sample sprite-group="SCN_RIGHT_LEG">
                <translation x="-10" y="-29" />
            </sample>
        </sample-set>
        <sample-set scene-node-id="SCN_RIGHT_FOOT">
            <sample sprite-group="SCN_RIGHT_FOOT">
                <translation x="3" y="-42" />
            </sample>
        </sample-set>
        <sample-set scene-node-id="SCN_LEFT_LEG">
            <sample sprite-group="SCN_LEFT_LEG">
                <translation x="7" y="-35" />
            </sample>
        </sample-set>
        <sample-set scene-node-id="SCN_LEFT_FOOT">
            <sample sprite-group="SCN_LEFT_FOOT">
                <translation x="21" y="-29" />
            </sample>
        </sample-set>
    </animation>
</animation-set>
<animation-set id="POP_ANM_SET">
    <animation id="POP_ANM_STAND" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_STAND" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_TURN">
        <sample-set>
            <sample sprite-group="POP_ANM_TURN" duration="0.0905" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_FALL" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_FALL" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_UP_START">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_UP_START" duration="0" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_UP" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_UP" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_HANG" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_HANG" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_CLIMB" rewind="1">
        <sample-set>
            <sample sprite-group="POP_ANM_CLIMB" duration="0.396" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_DESCEND">
        <sample-set>
            <sample sprite-group="POP_ANM_CLIMB" duration="0.396" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_FORWARD_START">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_FORWARD_START" duration="0" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_FORWARD" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_FORWARD" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_JUMP_FORWARD_END">
        <sample-set>
            <sample sprite-group="POP_ANM_JUMP_FORWARD_END" duration="0.132" />
        </sample-set>
    </animation>
    <animation id="POP_ANM_WALK" repeat="1">
        <sample-set>
            <sample sprite-group="POP_ANM_WALK" duration="0.396" />
        </sample-set>
    </animation>
</animation-set>