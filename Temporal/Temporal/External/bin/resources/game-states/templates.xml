<entity id="ENT_LASER">
	<transform />
	<collision-filter category="4" />
	<dynamic-body gravity-enabled="false">
		<fixture>
			<obb angle="0">
				<center x="0" y="0" />
				<radius x="4" y="1" />
			</obb>
		</fixture>
	</dynamic-body>
	<laser />
	<renderer layer="5" texture="resources/textures/laser.png">
		<color r="1.0" g="0.0" b="0.0" />
	</renderer>
	<temporal-echo />
</entity>
