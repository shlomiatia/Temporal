<animation-set id="DOR_ANM_SET">
	<animation id="DOR_ANM_OPEN" repeat="1">
        <scene-graph-sample>
            <scene-node-sample sprite-group="DOR_ANM_OPEN" />
        </scene-graph-sample>
    </animation>
	<animation id="DOR_ANM_OPENING">
        <scene-graph-sample index="0">
            <scene-node-sample sprite-group="DOR_ANM_OPENING" />
        </scene-graph-sample>
		<scene-graph-sample index="10" />
	</animation>
	<animation id="DOR_ANM_CLOSE" repeat="1">
        <scene-graph-sample>
            <scene-node-sample sprite-group="DOR_ANM_CLOSE" />
        </scene-graph-sample>
    </animation>
</animation-set>